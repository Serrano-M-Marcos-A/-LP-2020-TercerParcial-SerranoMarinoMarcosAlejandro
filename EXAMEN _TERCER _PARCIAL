		Console.WriteLine("numero 2");
		string b = Console.ReadLine();
		Console.WriteLine("numero 3");
		string c = Console.ReadLine();
    mayor= Int32.Parse(Console.ReadLine());
	menor= Int32.Parse(Console.ReadLine());
		switch (1)
		{

			case "mayor":

				Console.WriteLine("el numero mayor es ");
				break;

			case "menor":
				Console.WriteLine("el numero menor es");
				break;

			default:
					Console.WriteLine("Los numeros son iguales");
					break;
			}
	    }



static void Unocienasd()
 {
int i=0;
int detener=100;
for(i=0; i<=detener; i=i+1)
      {
    Console.WriteLine(+i);
       }
 return ;
}
static void Cienunodes()
{
int y=100;
int parar=0;
           for(y=100; y>=parar; y = y-1)
		{
		Console.WriteLine(+y);
		}
return ;
 }
static void Sumacien() 
{
int x=0;
int alto=100;
	for(x=1; x<=alto; x=x+1)
		{
			int n= 0;
			n=x+1;
			Console.WriteLine(x*n/2);
		}
}

static void Frases() {
int z=0;
int stop = 0;
int respuesta=0;
int contador=0;

		for(z=1; z>stop; z++)
		{
			Console.WriteLine("Ingrese frase");
			Console.ReadLine();
			contador++;
			Console.WriteLine("Quiere continuar? (1.-si/2.-no");
			respuesta = Int32.Parse(Console.ReadLine());

			if(respuesta==2)
			{
				Console.WriteLine("frase: "+contador);
				stop=1000;
			}
		}
return ;
}

static  void Sumaimpares()
{
int num1=0;
        int num2=0;
		int f=0;
		int pares=0;
		int impares=0;
		int numNat=0;
	    int contador = 0;
		num1=Int32.Parse(Console.ReadLine());
		num2=Int32.Parse(Console.ReadLine());
		Console.WriteLine(" " +num1);
		Console.WriteLine(" " +num2);
		for(f=num1+1; f<=num2-1; f++)
		{
			Console.WriteLine(+f);
			numNat=numNat+1;
			if (f==0)
			{
				pares=pares+1;
			}
			else
			{
				impares=impares+1;
			}
		}
		Console.WriteLine("Los numeros naturales son: " + numNat);
		Console.WriteLine("Los numeros pares son: " + contador);
		Console.WriteLine("La suma de numeros impares es: " +impares);
                         }
}




static void Unocienasdw() {
int i = 0;
		while(i<=100)
		{
			Console.WriteLine(i);
			i++;
		}
}

static void Ciencerodesw() {
int n = 100;
		while(n>=0)
		{
			Console.WriteLine(n);
			n =n-1;
		}
}

static void Sumacienw() {
int cont = 1;
		int suma = 0;
		while(cont<=100)
		{
			suma = suma + cont;
			Console.WriteLine(suma);
			cont++;
		}
}

static void Frasesw() {
bool stop = true;
		string respuesta = "";
		int contador = 0;
		while(stop==true)
		{
			Console.WriteLine("Ingrese una frase");
			Console.ReadLine();
			contador++;
			Console.WriteLine("Quiere ingresar otra frase?");
			respuesta = Console.ReadLine();

			if(respuesta == "no")
			{

				Console.WriteLine("El total de frases son: " + contador);
				stop = false;
			}
		}
} 

static void Sumaimpp() {

int m =0;
		int n=0;
		int y=0;
		int pr = 0;
		int sum = 0;
		Console.WriteLine("Introduzca el primer número");
		m = Int32.Parse(Console.ReadLine());
		Console.WriteLine("Introduzca el segundo número");
		n = Int32.Parse(Console.ReadLine());
		y = m+1;
		while(y<n)
		{
			Console.WriteLine(y);
			y++;
			if(y %2 == 0)
			{
				pr++;
			}
			else
			{

				sum = sum + y;
			}
		}
		Console.WriteLine("Los numeros pares son: " +pr);
		Console.WriteLine("La suma de los impares es: " +sum);
}
