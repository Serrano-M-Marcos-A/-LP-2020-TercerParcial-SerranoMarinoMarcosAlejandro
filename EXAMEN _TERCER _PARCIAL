static void Numerosdetresdiferentes() {

double  a,b,c; 
Console.WriteLine("ingrese numero A:");
a = double.Parse(Console.ReadLine()); 
Console.Clear(); Console.WriteLine("ingrese numero B:"); 
b = double.Parse(Console.ReadLine()); Console.Clear();
if(a==b) {
while (b==a)      {
Console.Clear();
Console.WriteLine("Ya ingreso ese numero antes ingrese otro \n ingrese numero B:");
b = double.Parse(Console.ReadLine()); Console.Clear();
		}


	}

Console.WriteLine("ingrese numero C:"); 
c = double.Parse(Console.ReadLine()); 
	if(c==b || c==a) {
	while (c==b || c==a) {
		Console.Clear();
		Console.WriteLine("Ya ingreso ese numero antes ingrese otro \n ingrese numero C:");
  			b = double.Parse(Console.ReadLine()); 
			Console.Clear();
    			      }

		          }


	if(a>=b && a>=c) {
		Console.WriteLine("El mayor es el numero A:"+a);
		}
              else if(b>c ) {
		Console.WriteLine("El mayor es el numero B:"+b);


		}
 else {
		Console.WriteLine("El mayor es el numero C:"+c);
}

if(a<=b && a<=c){

		Console.WriteLine("El Menor es el numero A:"+a);

		}
else
 if(b<c)   {
		Console.WriteLine("El Menor es el numero B:"+b);
		}
else    {
		Console.WriteLine("El Menor es el numero C:"+c);


		}
		return ;


	}

static void NumeroParOImpar()
{
int numpar = 0;
	Console.WriteLine("Determinaremos si un número entero es par o impar");
	Console.WriteLine("Ingresa un numero entero");

	numpar = Int32.Parse(Console.ReadLine());
	if(numpar % 2 == 0)
	{
	     Console.WriteLine("El numero es par");
	}
	else
	{
		Console.WriteLine("El numero es impar");
	}

} 	

static void Potencian()
{
	double numero,potencia,resultado; 
Console.WriteLine("ingrese numero"); 
numero = double.Parse(Console.ReadLine()); 
Console.WriteLine("ingrese potencia");
potencia = double.Parse(Console.ReadLine());
resultado=Math.Pow(numero, potencia);
Console.WriteLine("resultado de elevar "+ numero, " a la potencia" +potencia+ ": "+resultado);
}

static void Operaciones2numeros()
 {
        int Num1 = 0;
		int Num2 = 0;
		Console.WriteLine("Ingresa un número");
		Num1 = Int32.Parse( Console.ReadLine());
		Console.WriteLine("Escribe otro número: ");
		Num2 = Int32.Parse( Console.ReadLine()); 
		Console.WriteLine("Indica que operación quieres que realice el programa");
		Console.WriteLine("sumar");
		Console.WriteLine("restar");
		Console.WriteLine("multiplicar");
		Console.WriteLine("dividir");
		string Opera = "";
		Opera = Console.ReadLine();
		switch (Opera)
		{
			case "sumar":
				Console.WriteLine(Num1+Num2);
			break;
			case "restar":
				Console.WriteLine(Num1-Num2);
				break;
			case "multiplicar":
				Console.WriteLine(Num1*Num2);
				break;
			case "dividir":
				Console.WriteLine(Num1/Num2);
				break;
			default:
				Console.WriteLine("Sigue las instr ucciones e intentalo de nuevo");
				break;	
		}

return ;

}

static void Areas()
 {
int Num4 = 0;
int Num5 = 0;

		Console.WriteLine("Base");
		Num4 = int.Parse(Console.ReadLine());
		Console.WriteLine("Altura");
		Num5 = int.Parse(Console.ReadLine());
		Console.WriteLine("Escriba si quiere calcular area o perimetro");
		string Op = "";
		Op = Console.ReadLine();

		switch (Op)
		{
			case "area":
				Console.WriteLine(Num4*Num5/2);
				break;
			case "perimetro":
				Console.WriteLine(Num4*3);
				break;

			default:
				Console.WriteLine("Vuelve a intentalo");
				break;
		}

}

